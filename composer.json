{
    "name": "mehdibo/ft-client",
    "description": "Client library for Forty Two Intranet API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdi Bounya",
            "email": "mehdi.bounya@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "fortytwo",
        "ft",
        "42",
        "42 school",
        "1337",
